<!DOCTYPE html>
<html>
<head>
  <title>Parking</title>
  <%= stylesheet_link_tag  'http://netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css', media: 'all' %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if flash[:errors] %>
  <% flash_message = flash[:critical] || flash[:error] || flash[:notice]%>
  <div class='notice'>
    <%=h flash[:errors] %>
  </div>
<% end %>

<div class="container-fluid">
  <%= yield %>
</div>
<%= javascript_include_tag 'application' %>
<%= javascript_include_tag "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js" %>
</body>
</html>
